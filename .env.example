# Gemini API
GEMINI_API_KEY=your_gemini_api_key

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_APP_URL=your_hosted_app_url

# Storage Provider Configuration
# Options: supabase, s3, cloudflare_r2, azure
VITE_STORAGE_PROVIDER=supabase

# Supabase Storage (default)
VITE_STORAGE_BUCKET=hushkey-vault

# AWS S3 / Cloudflare R2 Configuration (optional)
# VITE_STORAGE_PROVIDER=s3
# VITE_STORAGE_BUCKET=your-bucket-name
# VITE_STORAGE_REGION=us-east-1
# VITE_STORAGE_ACCESS_KEY=your_access_key
# VITE_STORAGE_SECRET_KEY=your_secret_key

# Cloudflare R2 Configuration (optional)
# VITE_STORAGE_PROVIDER=cloudflare_r2
# VITE_STORAGE_BUCKET=your-bucket-name
# VITE_STORAGE_ENDPOINT=https://your-account-id.r2.cloudflarestorage.com
# VITE_STORAGE_ACCESS_KEY=your_access_key
# VITE_STORAGE_SECRET_KEY=your_secret_key
# VITE_STORAGE_ACCOUNT_ID=your_account_id
